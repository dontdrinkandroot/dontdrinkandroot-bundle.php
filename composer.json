{
    "name": "dontdrinkandroot/utils-bundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "~2.7",
        "dontdrinkandroot/utils": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "doctrine/orm": "~2.5",
        "doctrine/data-fixtures": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\UtilsBundle\\": ""
        }
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing"
    },
    "bin": [
        "bin/install",
        "bin/update",
        "bin/maintenance.off",
        "bin/maintenance.on",
        "bin/set-permissions"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
